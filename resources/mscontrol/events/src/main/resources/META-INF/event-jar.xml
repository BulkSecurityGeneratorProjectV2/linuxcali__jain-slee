<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE event-jar PUBLIC "-//Sun Microsystems, Inc.//DTD JAIN SLEE Event 1.1//EN" "http://java.sun.com/dtd/slee-event-jar_1_1.dtd">
<event-jar>
	<description>Event definitions for JCC 1.1 events</description>
        <library-ref>
            <library-name>msc-library</library-name>
            <library-vendor>org.mobicents.resources</library-vendor>
            <library-version>${pom.version}</library-version>
        </library-ref>
        
	<!-- Network connection events -->
        <event-definition>
            <event-type-name>javax.media.mscontrol.networkconnection.SdpPortManagerEvent.ANSWER_GENERATED</event-type-name>
            <event-type-vendor>org.mobicents</event-type-vendor>
            <event-type-version>1.0</event-type-version>
            <event-class-name>javax.media.mscontrol.networkconnection.SdpPortManagerEvent</event-class-name>
        </event-definition>
        
        <event-definition>
            <event-type-name>javax.media.mscontrol.networkconnection.SdpPortManagerEvent.ANSWER_PROCESSED</event-type-name>
            <event-type-vendor>org.mobicents</event-type-vendor>
            <event-type-version>1.0</event-type-version>
            <event-class-name>javax.media.mscontrol.networkconnection.SdpPortManagerEvent</event-class-name>
        </event-definition>

        <event-definition>
            <event-type-name>javax.media.mscontrol.networkconnection.SdpPortManagerEvent.NETWORK_STREAM_FAILURE</event-type-name>
            <event-type-vendor>org.mobicents</event-type-vendor>
            <event-type-version>1.0</event-type-version>
            <event-class-name>javax.media.mscontrol.networkconnection.SdpPortManagerEvent</event-class-name>
        </event-definition>
        
        <event-definition>
            <event-type-name>javax.media.mscontrol.networkconnection.SdpPortManagerEvent.OFFER_GENERATED</event-type-name>
            <event-type-vendor>org.mobicents</event-type-vendor>
            <event-type-version>1.0</event-type-version>
            <event-class-name>javax.media.mscontrol.networkconnection.SdpPortManagerEvent</event-class-name>
        </event-definition>

        <!-- Join/Unjoin Events --> 
        <event-definition>
            <event-type-name>javax.media.mscontrol.join.JoinEvent.JOINED</event-type-name>
            <event-type-vendor>org.mobicents</event-type-vendor>
            <event-type-version>1.0</event-type-version>
            <event-class-name>javax.media.mscontrol.join.JoinEvent</event-class-name>
        </event-definition>

        <event-definition>
            <event-type-name>javax.media.mscontrol.join.JoinEvent.UNJOINED</event-type-name>
            <event-type-vendor>org.mobicents</event-type-vendor>
            <event-type-version>1.0</event-type-version>
            <event-class-name>javax.media.mscontrol.join.JoinEvent</event-class-name>
        </event-definition>

        
        <!-- Media Player Events -->
        <event-definition>
            <event-type-name>javax.media.mscontrol.mediagroup.PlayerEvent.PLAY_COMPLETED</event-type-name>
            <event-type-vendor>org.mobicents</event-type-vendor>
            <event-type-version>1.0</event-type-version>
            <event-class-name>javax.media.mscontrol.mediagroup.PlayerEvent</event-class-name>
        </event-definition>
        <event-definition>
            <event-type-name>javax.media.mscontrol.mediagroup.PlayerEvent.PAUSED</event-type-name>
            <event-type-vendor>org.mobicents</event-type-vendor>
            <event-type-version>1.0</event-type-version>
            <event-class-name>javax.media.mscontrol.mediagroup.PlayerEvent</event-class-name>
        </event-definition>
        <event-definition>
            <event-type-name>javax.media.mscontrol.mediagroup.PlayerEvent.RESUMED</event-type-name>
            <event-type-vendor>org.mobicents</event-type-vendor>
            <event-type-version>1.0</event-type-version>
            <event-class-name>javax.media.mscontrol.mediagroup.PlayerEvent</event-class-name>
        </event-definition>
        <event-definition>
            <event-type-name>javax.media.mscontrol.mediagroup.PlayerEvent.SPEED_CHANGED</event-type-name>
            <event-type-vendor>org.mobicents</event-type-vendor>
            <event-type-version>1.0</event-type-version>
            <event-class-name>javax.media.mscontrol.mediagroup.PlayerEvent</event-class-name>
        </event-definition>
        <event-definition>
            <event-type-name>javax.media.mscontrol.mediagroup.PlayerEvent.VOLUME_CHANGED</event-type-name>
            <event-type-vendor>org.mobicents</event-type-vendor>
            <event-type-version>1.0</event-type-version>
            <event-class-name>javax.media.mscontrol.mediagroup.PlayerEvent</event-class-name>
        </event-definition>
        <!-- Recorder Events -->
        <event-definition>
            <event-type-name>javax.media.mscontrol.mediagroup.RecorderEvent.PAUSED</event-type-name>
            <event-type-vendor>org.mobicents</event-type-vendor>
            <event-type-version>1.0</event-type-version>
            <event-class-name>javax.media.mscontrol.mediagroup.RecorderEvent</event-class-name>
        </event-definition>
        <event-definition>
            <event-type-name>javax.media.mscontrol.mediagroup.RecorderEvent.RESUMED</event-type-name>
            <event-type-vendor>org.mobicents</event-type-vendor>
            <event-type-version>1.0</event-type-version>
            <event-class-name>javax.media.mscontrol.mediagroup.RecorderEvent</event-class-name>
        </event-definition>
        <event-definition>
            <event-type-name>javax.media.mscontrol.mediagroup.RecorderEvent.RECORD_COMPLETED</event-type-name>
            <event-type-vendor>org.mobicents</event-type-vendor>
            <event-type-version>1.0</event-type-version>
            <event-class-name>javax.media.mscontrol.mediagroup.RecorderEvent</event-class-name>
        </event-definition>
        <event-definition>
            <event-type-name>javax.media.mscontrol.mediagroup.RecorderEvent.STARTED</event-type-name>
            <event-type-vendor>org.mobicents</event-type-vendor>
            <event-type-version>1.0</event-type-version>
            <event-class-name>javax.media.mscontrol.mediagroup.RecorderEvent</event-class-name>
        </event-definition>
        
</event-jar>
