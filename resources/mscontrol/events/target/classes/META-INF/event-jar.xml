<?xml version="1.0" encoding="ISO-8859-1"?>
<!DOCTYPE event-jar PUBLIC "-//Sun Microsystems, Inc.//DTD JAIN SLEE Event 1.1//EN" "http://java.sun.com/dtd/slee-event-jar_1_1.dtd">
<event-jar>
	<description>Event definitions for JCC 1.1 events</description>
        <library-ref>
            <library-name>msc-library</library-name>
            <library-vendor>org.mobicents.resources</library-vendor>
            <library-version>3.0.0-SNAPSHOT</library-version>
        </library-ref>
	<!-- jcc connection events -->
        <event-definition>
            <event-type-name>javax.media.mscontrol.networkconnection.SdpPortManagerEvent.ANSWER_GENERATED</event-type-name>
            <event-type-vendor>org.mobicents</event-type-vendor>
            <event-type-version>1.0</event-type-version>
            <event-class-name>javax.media.mscontrol.networkconnection.SdpPortManagerEvent</event-class-name>
        </event-definition>
        
        <event-definition>
            <event-type-name>javax.media.mscontrol.networkconnection.SdpPortManagerEvent.ANSWER_PROCESSED</event-type-name>
            <event-type-vendor>org.mobicents</event-type-vendor>
            <event-type-version>1.0</event-type-version>
            <event-class-name>javax.media.mscontrol.networkconnection.SdpPortManagerEvent</event-class-name>
        </event-definition>

        <event-definition>
            <event-type-name>javax.media.mscontrol.networkconnection.SdpPortManagerEvent.NETWORK_STREAM_FAILURE</event-type-name>
            <event-type-vendor>org.mobicents</event-type-vendor>
            <event-type-version>1.0</event-type-version>
            <event-class-name>javax.media.mscontrol.networkconnection.SdpPortManagerEvent</event-class-name>
        </event-definition>
        
        <event-definition>
            <event-type-name>javax.media.mscontrol.networkconnection.SdpPortManagerEvent.OFFER_GENERATED</event-type-name>
            <event-type-vendor>org.mobicents</event-type-vendor>
            <event-type-version>1.0</event-type-version>
            <event-class-name>javax.media.mscontrol.networkconnection.SdpPortManagerEvent</event-class-name>
        </event-definition>
</event-jar>
