<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

	<modelVersion>4.0.0</modelVersion>

	<parent>
		<artifactId>mobicents-slee-ra-mscontrol</artifactId>
		<groupId>org.mobicents.resources.mscontrol</groupId>
    	<version>3.0.0-SNAPSHOT</version>
	</parent>

	<artifactId>mobicents-slee-ra-mscontrol-library</artifactId>

	<name>Mobicents :: Resources :: ${pom.artifactId}</name>

	<dependencies>
		<dependency>
			<groupId>org.mobicents.external.jsr309</groupId>
			<artifactId>mscontrol</artifactId>
			<version>1.0</version>
		</dependency>
		<dependency>
			<groupId>org.mobicents.jsr309</groupId>
			<artifactId>mobicents-jsr309-impl</artifactId>
			<version>2.0.0.BETA5-SNAPSHOT</version>
		</dependency>
		<dependency>
			<groupId>jain</groupId>
			<artifactId>jain-mgcp-ri</artifactId>
			<version>1.0</version>
		</dependency>
		<dependency>
			<groupId>org.mobicents.protocols.mgcp</groupId>
			<artifactId>mgcp-impl</artifactId>
			<version>2.0.0.GA</version>
		</dependency>
	</dependencies>


	<build>
		<plugins>
			<plugin>
				<groupId>org.mobicents.tools</groupId>
				<artifactId>maven-library-plugin</artifactId>
				<extensions>true</extensions>
				<executions>
					<execution>
						<phase>generate-resources</phase>
						<goals>
							<goal>create</goal>
						</goals>
					</execution>
				</executions>
				<configuration>
					<library-name>msc-library</library-name>
					<library-vendor>org.mobicents.resources</library-vendor>
					<library-version>${pom.version}</library-version>
				</configuration>
			</plugin>
		</plugins>
	</build>

</project>