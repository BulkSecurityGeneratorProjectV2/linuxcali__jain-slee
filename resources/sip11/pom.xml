<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	<modelVersion>4.0.0</modelVersion>
	<packaging>pom</packaging>

	<parent>
		<groupId>org.mobicents</groupId>
		<artifactId>mobicents-parent</artifactId>
		<version>2.12</version>
	</parent>
	
	<artifactId>mobicents-slee-ra-sip11</artifactId>
	<groupId>org.mobicents.resources</groupId>
	<version>3.0.0-SNAPSHOT</version>
	
	<name>Mobicents :: Resources :: ${pom.artifactId}</name>

	<modules>
		<module>jain-sip-ha</module>
		<module>library</module>
		<module>events</module>
		<module>ratype</module>
		<module>ra</module>
		<module>du</module>
	</modules>
	
	<properties>
		<mobicents.slee.version>2.2.0.FINAL</mobicents.slee.version>
		<jsip.ha.version>0.13</jsip.ha.version>
		<jsip.sip.ext.version>1.0.BETA1</jsip.sip.ext.version>
		<loadbalancer.version>1.2.FINAL</loadbalancer.version>
		<docs.profile>mobicents</docs.profile>
		<docs.ra.name>SIP11</docs.ra.name>
		<docs.ra.dirname>sip11</docs.ra.dirname>
		<docs.bookid>SIP11</docs.bookid>
	</properties>
	
	<dependencyManagement>
		<dependencies>
			<dependency>
					<groupId>org.mobicents.ha.javax.sip</groupId>
					<artifactId>mobicents-jain-sip-ha-core</artifactId>
					<version>${jsip.ha.version}</version>
			</dependency>
			<dependency>
				<groupId>org.mobicents.javax.sip</groupId>
				<artifactId>mobicents-jain-sip-ext</artifactId>
				<version>${jsip.sip.ext.version}</version>
			</dependency>
			<dependency>
					<groupId>org.mobicents.ha.javax.sip</groupId>
					<artifactId>mobicents-jain-sip-jboss5</artifactId>
					<version>${jsip.ha.version}</version>
			</dependency>
			<dependency>
				<groupId>org.mobicents.servers.jainslee.core</groupId>
				<artifactId>fault-tolerant-ra-api</artifactId>
				<version>${mobicents.slee.version}</version>
			</dependency>
			<dependency>
				<groupId>org.mobicents.servers.jainslee.core</groupId>
				<version>${mobicents.slee.version}</version>
				<artifactId>spi</artifactId>
			</dependency>
			<dependency>
				<groupId>org.mobicents.tools</groupId>
				<artifactId>sip-balancer-jar</artifactId>
				<version>${loadbalancer.version}</version>			
			</dependency>
		</dependencies>
	</dependencyManagement>
			
	<profiles>
		<profile>
			<id>jbcp</id>
			<properties>
				<docs.profile>jbcp</docs.profile>
			</properties>
		</profile>
			
		<profile>
			<id>release</id>
			<modules>
				<module>docs</module>
				<module>release</module>
			</modules>
		</profile>
		<profile>
			<id>maven-release</id>
			<modules>
				<module>docs</module>
				<module>release</module>
			</modules>
			<build>
				<plugins>
					<plugin>
					<groupId>org.apache.maven.plugins</groupId>
					<artifactId>maven-release-plugin</artifactId>
					<configuration>
						<autoVersionSubmodules>true</autoVersionSubmodules>
						<tagBase>https://mobicents.googlecode.com/svn/tags/servers/jain-slee/2.x.y/resources/sip11</tagBase>
						<branchBase>https://mobicents.googlecode.com/svn/branches/servers/jain-slee/2.x.y/resources/sip11</branchBase>
						<preparationGoals>clean install</preparationGoals>
					</configuration>
					</plugin>
				</plugins>
			</build>		
		</profile>
	</profiles>

	<scm>
		<connection>scm:svn:https://mobicents.googlecode.com/svn/trunk/servers/jain-slee/resources/sip11</connection>
		<developerConnection>scm:svn:https://mobicents.googlecode.com/svn/trunk/servers/jain-slee/resources/sip11</developerConnection>
		<url>http://mobicents.googlecode.com/svn/trunk/servers/jain-slee/resources/sip11</url>
	</scm>
	
<build>
		<plugins>
			<plugin>
				<artifactId>maven-eclipse-plugin</artifactId>
				<groupId>org.mobicents.tools</groupId>
				<inherited>false</inherited>
				<executions />
				<configuration>
					<excludePoms />
					<classpathExcludes>
						<exclude>xml-apis:xml-apis</exclude>
						<exclude>jtidy:jtidy</exclude>
					</classpathExcludes>
					<resolveTransitiveDependencies>true</resolveTransitiveDependencies>
					<eclipseProjectName>mobicents-slee-ra-sip11</eclipseProjectName>
				</configuration>
			</plugin>
		</plugins>		
	</build>
	
	<!-- dupe info from parent, but it's only way to retrieve the parent without checkout -->

	<repositories>
		<repository>
			<id>jboss-public-repository-group</id>
			<name>JBoss Public Maven Repository Group</name>
			<url>https://repository.jboss.org/nexus/content/groups/public</url>
			<layout>default</layout>
			<releases>
				<enabled>true</enabled>
				<updatePolicy>never</updatePolicy>
			</releases>
			<snapshots>
				<enabled>true</enabled>
				<updatePolicy>never</updatePolicy>
			</snapshots>
		</repository>
	</repositories>
	
</project>