<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE sbb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD JAIN SLEE SBB 1.1//EN"
                         "http://java.sun.com/dtd/slee-sbb-jar_1_1.dtd">

<sbb-jar>
	<sbb id="ra-bounce-sbb">
		<description>SecondBounceSbb for training example5</description>

		<sbb-name>SecondBounceSbb</sbb-name>
		<sbb-vendor>org.mobicents</sbb-vendor>
		<sbb-version>2.0</sbb-version>

		<library-ref>
			<library-name>example5-library</library-name>
			<library-vendor>org.mobicents.examples</library-vendor>
			<library-version>1.0</library-version>
		</library-ref>

		<sbb-classes>
			<sbb-abstract-class>
				<sbb-abstract-class-name>
					org.mobicents.slee.training.example5.second.SecondBounceSbb
				</sbb-abstract-class-name>
				<cmp-field>
					<cmp-field-name>nullActivity</cmp-field-name>
				</cmp-field>
			</sbb-abstract-class>
		</sbb-classes>

		<event event-direction="Receive" initial-event="True">
			<event-name>ReceiveMessage</event-name>
			<event-type-ref>
				<event-type-name>
					org.mobicents.slee.resource.lab.RequestEvent
				</event-type-name>
				<event-type-vendor>org.mobicents</event-type-vendor>
				<event-type-version>2.0</event-type-version>
			</event-type-ref>
			<initial-event-select variable="ActivityContext" />
		</event>

		<event event-direction="Fire">
			<event-name>SendMessage</event-name>
			<event-type-ref>
				<event-type-name>
					org.mobicents.slee.resource.lab.ResponseEvent
				</event-type-name>
				<event-type-vendor>org.mobicents</event-type-vendor>
				<event-type-version>2.0</event-type-version>
			</event-type-ref>
		</event>

	</sbb>

</sbb-jar>
