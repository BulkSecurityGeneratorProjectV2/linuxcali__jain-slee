<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE sbb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD JAIN SLEE SBB 1.1//EN" "http://java.sun.com/dtd/slee-sbb-jar_1_1.dtd">

<sbb-jar>
	<sbb id="ra-bounce-sbb">
		<description>Bounce SBB for training example9</description>

		<sbb-name>TrainingExample9</sbb-name>
		<sbb-vendor>org.mobicents</sbb-vendor>
		<sbb-version>2.0</sbb-version>
		<profile-spec-ref>
			<profile-spec-name>
				EventControlProfileCMP
			</profile-spec-name>
			<profile-spec-vendor>org.mobicents</profile-spec-vendor>
			<profile-spec-version>2.0</profile-spec-version>
			<profile-spec-alias>EventControlProfile</profile-spec-alias>
		</profile-spec-ref>

		<sbb-classes>
			<sbb-abstract-class>
				<sbb-abstract-class-name>
					org.mobicents.slee.training.example9.BounceSbb
				</sbb-abstract-class-name>
				<!-- get-profile-cmp-method>
					<profile-spec-alias-ref>
					EventControlProfile
					</profile-spec-alias-ref>
					<get-profile-cmp-method-name>
					getEventControlProfileCMP
					</get-profile-cmp-method-name>
					</get-profile-cmp-method -->
			</sbb-abstract-class>
		</sbb-classes>


		<event event-direction="Receive" initial-event="True">
			<event-name>ServiceStarted</event-name>
			<event-type-ref>
				<event-type-name>
					javax.slee.serviceactivity.ServiceStartedEvent
				</event-type-name>
				<event-type-vendor>javax.slee</event-type-vendor>
				<event-type-version>1.1</event-type-version>
			</event-type-ref>
			<initial-event-select variable="ActivityContext" />
		</event>

		<!-- JSLEE v1.1 Specification, Early Draft Review Page 25 -->
		<event event-direction="Receive" initial-event="True">
			<event-name>InitEvent</event-name>
			<event-type-ref>
				<event-type-name>
					org.mobicents.slee.resource.lab.message.incoming.INIT
				</event-type-name>
				<event-type-vendor>org.mobicents</event-type-vendor>
				<event-type-version>2.0</event-type-version>
			</event-type-ref>
			<initial-event-select variable="ActivityContext" />
		</event>

		<event event-direction="Receive" initial-event="False">
			<event-name>AnyEvent</event-name>
			<event-type-ref>
				<event-type-name>
					org.mobicents.slee.resource.lab.message.incoming.ANY
				</event-type-name>
				<event-type-vendor>org.mobicents</event-type-vendor>
				<event-type-version>2.0</event-type-version>
			</event-type-ref>
			<initial-event-select variable="ActivityContext" />
		</event>

		<event event-direction="Receive" initial-event="False">
			<event-name>EndEvent</event-name>
			<event-type-ref>
				<event-type-name>
					org.mobicents.slee.resource.lab.message.incoming.END
				</event-type-name>
				<event-type-vendor>org.mobicents</event-type-vendor>
				<event-type-version>2.0</event-type-version>
			</event-type-ref>
			<initial-event-select variable="ActivityContext" />
		</event>

		<resource-adaptor-type-binding>
			<resource-adaptor-type-ref>
				<resource-adaptor-type-name>
					LabResourceAdaptorType
				</resource-adaptor-type-name>
				<resource-adaptor-type-vendor>
					org.mobicents
				</resource-adaptor-type-vendor>
				<resource-adaptor-type-version>
					2.0
				</resource-adaptor-type-version>
			</resource-adaptor-type-ref>

			<activity-context-interface-factory-name>
				slee/resources/LabRA/raframeacif
			</activity-context-interface-factory-name>

			<resource-adaptor-entity-binding>
				<resource-adaptor-object-name>
					slee/resources/raframe/2.0/sbb2ra
				</resource-adaptor-object-name>
				<resource-adaptor-entity-link>
					LabRA
				</resource-adaptor-entity-link>
			</resource-adaptor-entity-binding>

		</resource-adaptor-type-binding>

	</sbb>

</sbb-jar>
