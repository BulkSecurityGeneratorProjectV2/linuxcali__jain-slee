<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE sbb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD JAIN SLEE SBB 1.1//EN"
                         "http://java.sun.com/dtd/slee-sbb-jar_1_1.dtd">
<sbb-jar>
	<sbb
		id="parentsbb">
		<description>Represents the entire call</description>
		<sbb-name>MapSbb</sbb-name>
		<sbb-vendor>org.mobicents</sbb-vendor>
		<sbb-version>1.0</sbb-version>
		<sbb-alias>MapSbb</sbb-alias>


		<sbb-classes>
			<sbb-abstract-class>
				<sbb-abstract-class-name>
					org.mobicents.slee.examples.map.MapSbb
				</sbb-abstract-class-name>

			</sbb-abstract-class>
		</sbb-classes>

		<event
			event-direction="Receive"
			initial-event="True">
			<event-name>OpenInfo</event-name>
			<event-type-ref>
				<event-type-name>
					org.mobicents.protocols.ss7.map.OPEN_INFO
				</event-type-name>
				<event-type-vendor>org.mobicents</event-type-vendor>
				<event-type-version>1.0</event-type-version>
			</event-type-ref>
			<initial-event-select
				variable="ActivityContext" />
		</event>

		<event
			event-direction="Receive"
			initial-event="False">
			<event-name>ProcessUnstructuredSSRequest</event-name>
			<event-type-ref>
				<event-type-name>
					org.mobicents.protocols.ss7.map.PROCESS_UNSTRUCTURED_SS_REQUEST_INDICATION
				</event-type-name>
				<event-type-vendor>org.mobicents</event-type-vendor>
				<event-type-version>1.0</event-type-version>
			</event-type-ref>
		</event>

		<event
			event-direction="Receive"
			initial-event="False">
			<event-name>UnstructuredSSRequest</event-name>
			<event-type-ref>
				<event-type-name>
					org.mobicents.protocols.ss7.map.UNSTRUCTURED_SS_REQUEST_INDICATION
				</event-type-name>
				<event-type-vendor>org.mobicents</event-type-vendor>
				<event-type-version>1.0</event-type-version>
			</event-type-ref>
		</event>

		<event
			event-direction="Receive"
			initial-event="False">
			<event-name>CloseInfo</event-name>
			<event-type-ref>
				<event-type-name>
					org.mobicents.protocols.ss7.map.CLOSE_INFO
				</event-type-name>
				<event-type-vendor>org.mobicents</event-type-vendor>
				<event-type-version>1.0</event-type-version>
			</event-type-ref>
		</event>

		<event
			event-direction="Receive"
			initial-event="False">
			<event-name>RefuseInfo</event-name>
			<event-type-ref>
				<event-type-name>
					org.mobicents.protocols.ss7.map.REFUSE_INFO
				</event-type-name>
				<event-type-vendor>org.mobicents</event-type-vendor>
				<event-type-version>1.0</event-type-version>
			</event-type-ref>
		</event>

		<event
			event-direction="Receive"
			initial-event="False">
			<event-name>UserAbortInfo</event-name>
			<event-type-ref>
				<event-type-name>
					org.mobicents.protocols.ss7.map.USER_ABORT_INFO
				</event-type-name>
				<event-type-vendor>org.mobicents</event-type-vendor>
				<event-type-version>1.0</event-type-version>
			</event-type-ref>
		</event>

		<event
			event-direction="Receive"
			initial-event="False">
			<event-name>ProviderAbortInfo</event-name>
			<event-type-ref>
				<event-type-name>
					org.mobicents.protocols.ss7.map.PROVIDER_ABORT_INFO
				</event-type-name>
				<event-type-vendor>org.mobicents</event-type-vendor>
				<event-type-version>1.0</event-type-version>
			</event-type-ref>
		</event>
		<resource-adaptor-type-binding>
			<resource-adaptor-type-ref>
				<resource-adaptor-type-name>
					MAPResourceAdaptorType
				</resource-adaptor-type-name>
				<resource-adaptor-type-vendor>
					org.mobicents
				</resource-adaptor-type-vendor>
				<resource-adaptor-type-version>
					2.0
				</resource-adaptor-type-version>
			</resource-adaptor-type-ref>
			<activity-context-interface-factory-name>
				slee/resources/map/2.0/acifactory
			</activity-context-interface-factory-name>
			<resource-adaptor-entity-binding>
				<resource-adaptor-object-name>
					slee/resources/map/2.0/provider
				</resource-adaptor-object-name>
				<resource-adaptor-entity-link>
					MAPRA
				</resource-adaptor-entity-link>
			</resource-adaptor-entity-binding>
		</resource-adaptor-type-binding>
	</sbb>
</sbb-jar>
