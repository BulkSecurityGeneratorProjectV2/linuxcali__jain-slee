<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">	
	
	<modelVersion>4.0.0</modelVersion>
		
	<parent>
		<artifactId>slee-connectivity-example-slee-parent</artifactId>
		<groupId>org.mobicents.examples</groupId>
		<version>2.1.0-SNAPSHOT</version>
	</parent>

	<artifactId>slee-connectivity-example-library</artifactId>
	<name>Mobicents Servers Examples :: SleeConnectivity :: ${pom.artifactId}</name>
	
	<dependencies>
		<dependency>
			<artifactId>event-pojo</artifactId>
			<groupId>org.mobicents.examples</groupId>
			<version>${pom.version}</version>
		</dependency>
		
	</dependencies>
	
	<build>
		<plugins>
			<plugin>
				<groupId>org.mobicents.tools</groupId>
				<artifactId>maven-library-plugin</artifactId>
				<extensions>true</extensions>
				<executions>
					<execution>
						<phase>generate-resources</phase>
						<goals>
							<goal>create</goal>
						</goals>
					</execution>
				</executions>
				<configuration>
					<library-name>slee-connectivity-example-library</library-name>
					<library-vendor>org.mobicents.examples</library-vendor>
					<library-version>1.0</library-version>
				</configuration>
			</plugin>
		</plugins>
	</build>
	
</project>