<?xml version="1.0" encoding="UTF-8"?>

<deployment xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="urn:jboss:bean-deployer:2.0">

	<bean name="SleeConnectionJMXLocal"
		class="org.mobicents.example.slee.connection.SleeConnectionTest">
		<annotation>@org.jboss.aop.microcontainer.aspects.jmx.JMX(name="jboss.mobicents:name=JMXAccessTest",exposedInterface=org.mobicents.example.slee.connection.SleeConnectionTestMBean.class,registerDirectly=true)
		</annotation>

		<property name="transactionManager">
			<inject bean="RealTransactionManager" />
		</property>
		

		<demand>RealTransactionManager</demand>
		
	</bean>

</deployment>
