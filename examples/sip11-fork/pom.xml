<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
	
	<modelVersion>4.0.0</modelVersion>
	
	<packaging>pom</packaging>
	
	<parent>
		<groupId>org.mobicents</groupId>
		<artifactId>mobicents-parent</artifactId>
		<version>2.19-SNAPSHOT</version>
	</parent>
	
	<artifactId>mobicents-slee-example-sip11-fork</artifactId>
	<groupId>org.mobicents.examples</groupId>
	<version>2.0.0-SNAPSHOT</version>
				
	<modules>
		<module>sbb</module>						
		<module>du</module>			
	</modules>
	
	<properties>
		<mobicents.resources.sip11.version>2.0.0.GA</mobicents.resources.sip11.version>
	</properties>

	<dependencyManagement>
		<dependencies>
			<dependency>
				<groupId>org.mobicents.resources</groupId>
				<artifactId>sip11-ratype</artifactId>
				<version>${mobicents.resources.sip11.version}</version>
			</dependency>
		</dependencies>
	</dependencyManagement>
	
	<profiles>
		<profile>
			<id>maven-release</id>
			<build>
				<plugins>
					<plugin>
					<groupId>org.apache.maven.plugins</groupId>
					<artifactId>maven-release-plugin</artifactId>
					<configuration>
						<autoVersionSubmodules>true</autoVersionSubmodules>
						<tagBase>https://mobicents.googlecode.com/svn/tags/servers/jain-slee/2.x.y/examples/sip11-fork</tagBase>
					</configuration>
					</plugin>
				</plugins>
			</build>		
		</profile>
	</profiles>	
	
	<scm>
		<connection>scm:svn:https://mobicents.googlecode.com/svn/trunk/servers/jain-slee/examples/sip11-fork</connection>
		<developerConnection>scm:svn:https://mobicents.googlecode.com/svn/trunk/servers/jain-slee/examples/sip11-fork</developerConnection>
		<url>http://mobicents.googlecode.com/svn/trunk/servers/jain-slee/examples/sip11-fork</url>
	</scm>

	<!-- dupe info from parent, but it's only way to retrieve the parent without checkout -->

	<repositories>
		<repository>
			<id>jboss</id>
			<name>JBoss Repository</name>
			<url>http://repository.jboss.org/maven2</url>
			<snapshots>
				<enabled>false</enabled>
			</snapshots>
		</repository>
		<repository>
			<id>jboss-snapshots</id>
			<name>JBoss Snapshot Repository</name>
			<url>http://snapshots.jboss.org/maven2</url>
			<releases>
				<enabled>false</enabled>
			</releases>
			<snapshots>
				<enabled>true</enabled>
			</snapshots>
		</repository>
	</repositories>
</project>
