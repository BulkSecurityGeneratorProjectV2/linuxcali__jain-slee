<?xml version="1.0"?>

<!DOCTYPE sbb-jar PUBLIC "-//Sun Microsystems, Inc.//DTD JAIN SLEE SBB 1.0//EN" "http://java.sun.com/dtd/slee-sbb-jar_1_0.dtd">

<sbb-jar>
    <sbb id="vpn-sbb">
        <description>Mobile Virtual Private Network</description>
        <sbb-name>VoiceVpnSBB</sbb-name>
        <sbb-vendor>mobicents</sbb-vendor>
        <sbb-version>1.0</sbb-version>
        <sbb-alias>VoiceVpnSBB</sbb-alias>
                
        <sbb-classes>
            <sbb-abstract-class>
                <sbb-abstract-class-name>
                    org.mobicents.examples.jcc.vpn.slee.VoiceVpnSbb              
                </sbb-abstract-class-name>
            </sbb-abstract-class> 
            
            <sbb-activity-context-interface>
                <sbb-activity-context-interface-name>
                    org.mobicents.examples.jcc.vpn.slee.ConnectionActivityContext
                </sbb-activity-context-interface-name>
            </sbb-activity-context-interface>
            
            
        </sbb-classes>
        
        <event event-direction="Receive" initial-event="True">
            <event-name>AuthorizeCallAttempt</event-name>
            <event-type-ref>
                <event-type-name>javax.csapi.cc.jcc.JccConnectionEvent.CONNECTION_AUTHORIZE_CALL_ATTEMPT</event-type-name>
                <event-type-vendor>javax.csapi.cc.jcc</event-type-vendor>
                <event-type-version>1.1</event-type-version>
            </event-type-ref>
            <initial-event-select variable="ActivityContext"/>
            <event-resource-option>block</event-resource-option>
        </event>        
        <event event-direction="Receive" initial-event="True">
            <event-name>AddressAnalyze</event-name>
            <event-type-ref>
                <event-type-name>javax.csapi.cc.jcc.JccConnectionEvent.CONNECTION_ADDRESS_ANALYZE</event-type-name>
                <event-type-vendor>javax.csapi.cc.jcc</event-type-vendor>
                <event-type-version>1.1</event-type-version>
            </event-type-ref>
            <initial-event-select variable="ActivityContext"/>
            <event-resource-option>block</event-resource-option>
        </event>        
        <event event-direction="Receive" initial-event="False">
            <event-name>ConnectionConnected</event-name>
            <event-type-ref>
                <event-type-name>javax.csapi.cc.jcc.JccConnectionEvent.CONNECTION_CONNECTED</event-type-name>
                <event-type-vendor>javax.csapi.cc.jcc</event-type-vendor>
                <event-type-version>1.1</event-type-version>
            </event-type-ref>
        </event>        
        <event event-direction="Receive" initial-event="False">
            <event-name>ConnectionDisconnected</event-name>
            <event-type-ref>
                <event-type-name>javax.csapi.cc.jcc.JccConnectionEvent.CONNECTION_DISCONNECTED</event-type-name>
                <event-type-vendor>javax.csapi.cc.jcc</event-type-vendor>
                <event-type-version>1.1</event-type-version>
            </event-type-ref>
        </event>        
        
		<resource-adaptor-type-binding>
			<resource-adaptor-type-ref>
				<resource-adaptor-type-name>JCC-1.1-RA</resource-adaptor-type-name>
				<resource-adaptor-type-vendor>javax.csapi.cc.jcc</resource-adaptor-type-vendor>
				<resource-adaptor-type-version>1.1</resource-adaptor-type-version>
			</resource-adaptor-type-ref>
			<activity-context-interface-factory-name>
				slee/resources/jcc/1.1/acifactory
			</activity-context-interface-factory-name>
			<resource-adaptor-entity-binding>
				<resource-adaptor-object-name>
					slee/resources/jcc/1.1/provider
				</resource-adaptor-object-name>
				<resource-adaptor-entity-link>
					JCCRA
				</resource-adaptor-entity-link>
			</resource-adaptor-entity-binding>
		</resource-adaptor-type-binding>
    </sbb> 
    
</sbb-jar>