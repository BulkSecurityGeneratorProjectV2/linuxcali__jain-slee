<?xml version="1.0" encoding="UTF-8"?>
<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">
    <modelVersion>4.0.0</modelVersion>
    
    <parent>
	<artifactId>ivr-demo-service-sip</artifactId>
	<groupId>org.mobicents.examples</groupId>
	<version>1.0.0-SNAPSHOT</version>
    </parent>
    
    <artifactId>ivr-demo-service-sip-sbb</artifactId>
    <groupId>org.mobicents.examples</groupId>
    <version>1.0.0-SNAPSHOT</version>
    
    <name>Interactive Voice Response [SIP,SBB]</name>
    <dependencies>
        <dependency>
            <groupId>javax.sip</groupId>
            <artifactId>jain-sip-api</artifactId>
            <version>1.2</version>
        </dependency>
        <dependency>
            <groupId>org.mobicents.resources</groupId>
            <artifactId>sip11-ratype</artifactId>
            <version>2.0.0.GA</version>
        </dependency>
        <dependency>
            <groupId>org.mobicents.resources</groupId>
            <artifactId>mgcp-ratype</artifactId>
            <version>2.0.0.GA</version>
        </dependency>
        <dependency>
            <groupId>javax.slee</groupId>
            <artifactId>jain-slee</artifactId>
            <version>1.1</version>
        </dependency>
        <dependency>
            <groupId>jain</groupId>
            <artifactId>jain-mgcp-ri</artifactId>
            <version>1.0</version>
        </dependency>
        <dependency>
            <artifactId>ivr-demo-service-events</artifactId>
            <groupId>org.mobicents.examples</groupId>
            <version>1.0.0-SNAPSHOT</version>
        </dependency>
    </dependencies>
    <build>
        <plugins>
            <plugin>
                <artifactId>maven-compiler-plugin</artifactId>
                <version>2.0.2</version>
                <configuration>
                    <source>1.5</source>
                    <target>1.5</target>
                </configuration>
            </plugin>
        </plugins>
    </build>
</project>
