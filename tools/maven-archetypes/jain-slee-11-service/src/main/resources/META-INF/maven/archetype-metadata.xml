<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor name="jain-slee-basic-service">
	<requiredProperties>
		<!-- 
		<requiredProperty key="mobicentsVersion">
			<defaultValue>2.0.0.GA</defaultValue>
		</requiredProperty>
		 -->
		 <requiredProperty key="mobicentsParentVersion">
			<defaultValue>2.4</defaultValue>
		</requiredProperty>
		<requiredProperty key="mobicentsEclipsePluginVersion">
			<defaultValue>1.0.0.BETA2</defaultValue>
		</requiredProperty>
		<requiredProperty key="mobicentsDUPluginVersion">
			<defaultValue>2.0.0.BETA3</defaultValue>
		</requiredProperty>
		<requiredProperty key="mobicentsLibraryPluginVersion">
			<defaultValue>1.0.0.BETA1</defaultValue>
		</requiredProperty>
	</requiredProperties>

	<fileSets>
		<fileSet filtered="true" packaged="false">
			<directory>du</directory>
			<includes>
				<include>**/*</include>
			</includes>
		</fileSet>
		<fileSet filtered="true" packaged="false">
			<directory>library</directory>
			<includes>
				<include>**/*</include>
			</includes>
		</fileSet>

		<fileSet filtered="true" packaged="false">
			<directory>events</directory>
			<includes>
				<include>**/*.xml</include>
			</includes>

		</fileSet>
		<fileSet filtered="true" packaged="true">
			<directory>events/src/main/java</directory>
			<includes>
				<include>**/*.java</include>
			</includes>
			
		</fileSet>
		<!-- SBB -->
		<fileSet filtered="true" packaged="false">
			<directory>sbb</directory>
			<includes>
				<include>**/*.xml</include>
			</includes>
		
		</fileSet>
		<fileSet filtered="true" packaged="true">
			<directory>sbb/src/main/java</directory>
			<includes>
				<include>**/*.java</include>
			</includes>
			
		</fileSet>
	</fileSets>
</archetype-descriptor>